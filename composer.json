{
    "name": "grechanyuk/favorite",
    "description": "Favorite, blacklist and other lists fo Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Egor G.",
            "email": "xorolllo@hotmail.com"
        }
    ],
    "homepage": "https://github.com/grechanyuk/favorite",
    "keywords": ["Laravel", "Favorite"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Grechanyuk\\Favorite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grechanyuk\\Favorite\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grechanyuk\\Favorite\\FavoriteServiceProvider"
            ]
        }
    }
}
